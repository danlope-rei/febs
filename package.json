{
  "name": "@rei/febs",
  "version": "4.2.0",
  "description": "REI's next-gen front-end build system.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "init": "./bin/init",
    "test:lib": "NODE_ENV=dev mocha -t 8000 test/lib-spec.js",
    "test:dev": "NODE_ENV=dev mocha -t 8000 test/index-dev-*spec.js",
    "test:css": "NODE_ENV=dev mocha -t 8000 test/index-dev-css-spec.js",
    "test:prod": "NODE_ENV=prod mocha -t 8000 test/index-prod-spec.js",
    "test": "npm run test:dev && npm run test:prod && npm run test:lib",
    "test-watch": "mocha --watch",
    "test-inspect": "mocha --inspect-brk -t 8000",
    "cover:prod": "NODE_ENV=prod istanbul cover node_modules/.bin/_mocha -- -t 8000 test/index-prod-spec.js",
    "cover:dev": "NODE_ENV=dev istanbul cover node_modules/.bin/_mocha -- -t 8000 test/index-dev-spec.js",
    "cover": "npm run cover:dev && npm run cover:prod",
    "build": "febs build",
    "dev": "webpack-dev-server --config src/webpack.dev.conf.js",
    "ci:build": "./node_modules/mocha/bin/_mocha --reporter xunit > results.xml && npm run cover",
    "lint": "./node_modules/.bin/eslint webpack-config lib ./index.js"
  },
  "bin": {
    "febs": "./bin/febs"
  },
  "keywords": [],
  "license": "MIT",
  "repository": "https://github.com/rei/febs",
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "ajv": "^6.10.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "commander": "^2.15.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.3",
    "istanbul": "~0.4.5",
    "less": "^2.3.1",
    "less-loader": "~4.1.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "ramda": "^0.25.0",
    "sass-loader": "^6.0.7",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "vue-loader": "^15.7.0",
    "vue-server-renderer": "^2.5.17",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.2",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "sinon": "^6.1.4",
    "vue": "^2.5.17"
  },
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "contributors": [
    {
      "name": "Alex Perkins",
      "email": "aperkin@rei.com"
    },
    {
      "name": "DeShawn Williams",
      "email": "dewilli@rei.com"
    }
  ]
}
